<table class="table">
  <thead>
   <th>ID</th>
   <th>Branch</th>
   <th>Description</th>
   <th></th>
  </thead>
  <tbody>
  <tr>
   <td><%= @driver_drop.id %></td>
   <td><%= @driver_drop.branch %></td>
   <td><%= @driver_drop.description %></td>
   <td>
    <div class="btn-group">
     <button class="btn btn-danger">Work On Drop Here</button>
     <button class="btn btn-danger dropdown-toggle" data-toggle="dropdown">
     <span class="caret"></span>
     </button>
       <ul class="dropdown-menu">
       <li>
        <a href= <%= new_package_path %> >Package Diver Drop</a>
       </li>
       <li>
        <a href="#">Create Drop Mail</a>
       </li>
       <li>
        <a href= <%= new_driver_drop_log_path(@driver_drop)%> >Upload Test Logs</a>
       </li>
       <li>
       <li class="divider"></li>
       <li>
        <li>
        <a href= <%= edit_driver_drop_path(@driver_drop) %> >Edit Drop Details</a>
       </li>
       </ul>
    </div>
   </td>
   </tr>
 </tbody>
</table>
<br>
<br>
<br>
<br>
<br>

<table class="table">
<thead>
  <th>x86 Failures</th>
  <th>Command Line</th>
  <th>Bug Number</th>
</thead>
<tbody>
  <tr>
   <% @x86log.each do |x| %> 
    <tr><td> id:<%= x.id %>  <%= x.name %>  </td><td>    <%= x.cmdline %></td><td></td></tr>
    <% end %>
  </tr>
</tbody>
</table>

<table class="table">
<thead>
    <th>x64 Failures</th>
    <th>Command Line</th>
    <th>Bug Number</th>
</thead>
<tbody>
  <tr>
   <% @x64log.each do |x| %> 
   <tr> <td> id:<%= x.id %>  <%= x.name %></td><td><%= x.cmdline %></td><td></td></tr>
    <% end %>   
  </tr>
</tbody>
</table>
